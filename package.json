  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Use yarn to install, instead of npm')\"",
    "start": "cross-env NODE_ENV=development node ./scripts/dev-server.js",
    "start:api": "API=true yarn start",
    "start:dashboard": "webpack-dashboard yarn start",
    "jest": "cross-env NODE_ICU_DATA=node_modules/full-icu TZ='Asia/Kolkata' NODE_ENV=test jest --config=./config/jest.config.js",
    "verify": "yarn install --check-files",
    "prepush:check": "yarn verify && yarn prettier && yarn lint",
    "test": "yarn verify && yarn jest",
    "test:prepush": "yarn jest --bail --silent",
    "test:file": "yarn jest --collectCoverage=false",
    "test:watch": "yarn jest --verbose --watchAll --coverageReporters='text-summary'",
    "tdd": "cross-env JEST_TDD=true yarn jest --verbose --watch",
    "prettier": "prettier \"{src, scripts, tests}/**/*.{js,md}\" --check",
    "prettier:fix": "prettier \"{src, scripts, tests}/**/*.{js,md}\" --write",
    "lint": "eslint --cache --config ./config/eslint.config.js ./src/",
    "lint:css": "stylelint './src/**/*.styles.js' './src/**/*.style.js'",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:error:fix": "yarn lint --quiet --fix",
    "lint:error:nocache": "eslint --config ./config/eslint.config.js --quiet ./src/",
    "ci": "yarn prettier && yarn lint:error:nocache",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack/prod.config.js",
    "open:coverage": "open ./coverage/lcov-report/index.html",
    "todo": "yarn leasot -x -r table \"src/**/*.js\"",
    "check-update": "yarn upgrade-interactive"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --cache --config ./config/eslint.config.js"
    ],
    "src/**/*.styles.js": [
      "prettier --write",
      "stylelint"
    ],
    "src/**/*.test.js": [
      "prettier --write",
      "eslint --cache --config ./config/eslint.config.js",
      "yarn test --collectCoverage=false"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn prepush:check && yarn test:prepush"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest-stare": {
    "coverageLink": "../coverage/lcov-report/index.html",
    "hidePassing": true,
    "hidePending": true,
    "resultDir": "test-reposts"
  }
}
